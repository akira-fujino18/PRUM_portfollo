<%= provide(:title, 'login') %>

<div class = "login_form">
  
  <h1>ログイン</h1>
  
    <%= form_with(url: login_path, scope: :session) do |f| %>
    
    <%= f.label :email, 'メールアドレス' %>
    <%= f.email_field :email, class: 'form_design' %>
    
    <%= f.label :password, 'パスワード'%>
    <%= f.password_field :password, class: 'form_design' %>
  
  </div> 
    <%= f.submit 'ログインする', class: 'loginning btn' %>
    <% end %>
    
   
